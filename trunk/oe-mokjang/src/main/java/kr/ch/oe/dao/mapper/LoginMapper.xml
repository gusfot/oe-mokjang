<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ch.oe.dao.LoginMapper">

	<select id="login" parameterType="kr.ch.oe.model.Member" resultType="int">
	  select count(id) from oe_member where email=#{email} and password=#{password}
	</select>

</mapper>